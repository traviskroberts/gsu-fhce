<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
    <servlet>
        <servlet-name>member_register</servlet-name>
        <servlet-class>gsu.member_register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>gsu.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>news</servlet-name>
        <servlet-class>gsu.news</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>calendar</servlet-name>
        <servlet-class>gsu.calendar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>resume</servlet-name>
        <servlet-class>gsu.resume</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>residency</servlet-name>
        <servlet-class>gsu.residency</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin</servlet-name>
        <servlet-class>gsu.admin</servlet-class>
    </servlet>
    <servlet><servlet-name>mentor</servlet-name><servlet-class>gsu.mentor</servlet-class></servlet><servlet><servlet-name>logout</servlet-name><servlet-class>gsu.logout</servlet-class></servlet>
    <servlet>
        <servlet-name>my_account</servlet-name>
        <servlet-class>gsu.my_account</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>member_register</servlet-name>
        <url-pattern>/member_register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>news</servlet-name>
        <url-pattern>/news</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>calendar</servlet-name>
        <url-pattern>/calendar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>resume</servlet-name>
        <url-pattern>/resume</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>residency</servlet-name>
        <url-pattern>/residency</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>mentor</servlet-name>
        <url-pattern>/mentor</url-pattern>
    </servlet-mapping>
    <servlet-mapping><servlet-name>logout</servlet-name><url-pattern>/logout</url-pattern></servlet-mapping>
    <servlet-mapping>
        <servlet-name>my_account</servlet-name>
        <url-pattern>/my_account</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
	<welcome-file>
            index.jsp
        </welcome-file>
    </welcome-file-list>
</web-app>
