<table class="register_info" cellspacing="5">
    <tr>
        <td>
            First Name: <input type="text" name="fname" id="fname"
                <% if(student.getFname() != null) { %>
                    value="<%= student.getFname() %>"
                <% } else { %>
                    value="" 
                <% } %>/>
        </td>
        <td>
            Last Name: <input type="text" name="lname" id="lname" 
                <% if(student.getLname() != null) { %>
                    value="<%= student.getLname() %>"
                <% } else { %>
                    value="" 
                <% } %>/>
        </td>
    </tr>
    <tr>
        <td colspan="2">
            Address:<br />
            <input type="text" name="address1" id="address1" 
                <% if(student.getAddress1() != null) { %>
                    value="<%= student.getAddress1() %>"
                <% } else { %>
                    value="" 
                <% } %>/><br />
            <input type="text" name="address2" id="address2" 
                <% if(student.getAddress2() != null) { %>
                    value="<%= student.getAddress2() %>"
                <% } else { %>
                    value="" 
                <% } %>/>
        </td>
    </tr>
    <tr>
        <td colspan="2">
            City: <input type="text" name="city" id="city" 
                <% if(student.getCity() != null) { %>
                    value="<%= student.getCity() %>"
                <% } else { %>
                    value="" 
                <% } %>/>
        </td>
    </tr>
    <tr>
        <td colspan="2">
            State: <input type="text" name="state" id="state" 
                <% if(student.getState() != null) { %>
                    value="<%= student.getState() %>"
                <% } else { %>
                    value="" 
                <% } %>/>
        </td>
    </tr>
    <tr>
        <td colspan="2">
            Zip: <input type="text" name="zip" id="zip" 
                <% if(student.getZip() != null) { %>
                    value="<%= student.getZip() %>"
                <% } else { %>
                    value="" 
                <% } %>/>
        </td>
    </tr>
    <tr>
        <td colspan="2">
            Email Address (this will be your username): <input type="text" name="email" id="email" 
                <% if(student.getEmail() != null) { %>
                    value="<%= student.getEmail() %>"
                <% } else { %>
                    value="" 
                <% } %>/>
        </td>
    </tr>
    <% if(student.getPassword() == null) { %>
    <tr>
        <td colspan="2">Password: <input type="password" name="password" id="pass" /></td>
    </tr>
    <% } %>
</table>
<table class="register_info" cellspacing="5">
    <tr>
        <td>
            Start Date: <input type="text" name="start_date" id="start_date" 
                <% if(student.getStartDate() != null) { %>
                    value="<%= student.getStartDate() %>"
                <% } else { %>
                    value="" 
                <% } %>/>
        </td>
    </tr>
    <tr>
        <td>
            Graduation Date: <input type="text" name="graduation_date" id="graduation_date" 
                <% if(student.getGraduationDate() != null) { %>
                    value="<%= student.getGraduationDate() %>"
                <% } else { %>
                    value="" 
                <% } %>/>
        </td>
    </tr>
</table>